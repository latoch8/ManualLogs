        <Button Grid.Row="19" Grid.Column="4" x:Uid="Pass"/>
        <Button Grid.Row="20" Grid.Column="4" x:Uid="Fail"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="35,10,0,0" Grid.Row="21" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <!--#endregion-->
    </Grid>

</Window>
