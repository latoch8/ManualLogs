        <TextBox Grid.Row="7" Grid.Column="1"/>
        <ComboBox Grid.Row="8" Grid.Column="1"/>
        <Button Grid.Row="9" Grid.Column="1" x:Uid="Pass"/>
        <Button Grid.Row="10" Grid.Column="1" x:Uid="Fail"/>
        <Label Grid.Row="11" Grid.Column="1"/>
        <TextBox Grid.Row="12" Grid.Column="1"/>
        <ComboBox Grid.Row="13" Grid.Column="1"/>
        <Button Grid.Row="14" Grid.Column="1" x:Uid="Pass"/>
        <Button Grid.Row="15" Grid.Column="1" x:Uid="Fail"/>
        <Label Grid.Row="16" Grid.Column="1"/>
        <TextBox Grid.Row="17" Grid.Column="1"/>
        <ComboBox Grid.Row="18" Grid.Column="1"/>
        <Button Grid.Row="19" Grid.Column="1" x:Uid="Pass"/>
        <Button Grid.Row="20" Grid.Column="1" x:Uid="Fail"/>

        <Label Grid.Row="1" Grid.Column="2"/>
        <TextBox Grid.Row="2" Grid.Column="2"/>
        <ComboBox Grid.Row="3" Grid.Column="2"/>
        <Button Grid.Row="4" Grid.Column="2" x:Uid="Pass"/>
        <Button Grid.Row="5" Grid.Column="2" x:Uid="Fail"/>
        <Label Grid.Row="6" Grid.Column="2"/>
        <TextBox Grid.Row="7" Grid.Column="2"/>
        <ComboBox Grid.Row="8" Grid.Column="2"/>
        <Button Grid.Row="9" Grid.Column="2" x:Uid="Pass"/>
        <Button Grid.Row="10" Grid.Column="2" x:Uid="Fail"/>
        <Label Grid.Row="11" Grid.Column="2"/>
        <TextBox Grid.Row="12" Grid.Column="2"/>
        <ComboBox Grid.Row="13" Grid.Column="2"/>
        <Button Grid.Row="14" Grid.Column="2" x:Uid="Pass"/>
        <Button Grid.Row="15" Grid.Column="2" x:Uid="Fail"/>
        <Label Grid.Row="16" Grid.Column="2"/>
        <TextBox Grid.Row="17" Grid.Column="2"/>
        <ComboBox Grid.Row="18" Grid.Column="2"/>
        <Button Grid.Row="19" Grid.Column="2" x:Uid="Pass"/>
        <Button Grid.Row="20" Grid.Column="2" x:Uid="Fail"/>

        <Label Grid.Row="1" Grid.Column="3"/>
        <TextBox Grid.Row="2" Grid.Column="3"/>
        <ComboBox Grid.Row="3" Grid.Column="3"/>
        <Button Grid.Row="4" Grid.Column="3" x:Uid="Pass"/>
